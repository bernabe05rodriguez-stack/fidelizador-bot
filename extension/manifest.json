{
  "manifest_version": 3,
  "name": "Bot Fidelizador",
  "version": "1.1",
  "permissions": ["storage", "tabs"],
  "host_permissions": ["https://web.whatsapp.com/*", "http://fidelizador.online/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": { "default_popup": "popup.html" },
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["content.js"]
    }
  ]
}